<% include partials/header %>

<h1>Posts:</h1>

<ul>
<% for (var i = 0; i < posts.length; i++) { %>
  <li><%= posts[i].title %> - <strong><%= posts[i].author %></strong></li>
<% } %>
</ul>

<ul>
  <% posts.forEach((post) => { %>
    <li><%= post.title %> - <strong><%= post.author %></strong></li>
  <% }); %>
</ul>

<% include partials/footer %>
