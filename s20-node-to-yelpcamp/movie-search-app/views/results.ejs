<% include partials/header %>

<h1>This is the result page!</h1>

<% if (results["Response"] === 'True') { %>
  <ul>
  <% results["Search"].forEach((movie) => { %>
    <li>
      <strong><%= movie["Title"] %></strong> - <%= movie["Year"] %>
    </li>
  <% }) %>
  </ul>
<% } else { %>
  <p>
    No movies found!
  </p>
<% } %>

<a href="/">Search again!</a>

<% include partials/footer %>
